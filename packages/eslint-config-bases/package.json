{
  "name": "@devlaunchers/eslint-config-bases",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/dev-launchers/dev-launchers-platform",
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-launchers/dev-launchers-platform",
    "directory": "packages/eslint-config-bases"
  },
  "author": {
    "name": "devlaunchers",
    "url": "https://github.com/dev-launchers"
  },
  "type": "commonjs",
  "main": "./index.js",
  "exports": {
    ".": {
      "require": "./src/index.js"
    },
    "./patch/modern-module-resolution": {
      "require": "./src/patch/modern-module-resolution.js"
    },
    "./helpers": {
      "require": "./src/helpers/index.js",
      "import": "./src/helpers/index.js"
    },
    "./jest": {
      "require": "./src/bases/jest.js"
    },
    "./playwright": {
      "require": "./src/bases/playwright.js"
    },
    "./prettier": {
      "require": "./src/bases/prettier.js"
    },
    "./react": {
      "require": "./src/bases/react.js"
    },
    "./rtl": {
      "require": "./src/bases/rtl.js"
    },
    "./regexp": {
      "require": "./src/bases/regexp.js"
    },
    "./sonar": {
      "require": "./src/bases/sonar.js"
    },
    "./storybook": {
      "require": "./src/bases/storybook.js"
    },
    "./typescript": {
      "require": "./src/bases/typescript.mjs",
      "import": "./src/bases/typescript.mjs"
    },
    "./tailwind": {
      "require": "./src/bases/tailwind.js"
    }
  },
  "scripts": {
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint  --ext .ts,.js,.cjs,.mjs --cache --cache-location ../../.cache/eslint/eslint-config-bases.eslintcache",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "fix-all-files": "eslint  --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "8.5.0",
    "@typescript-eslint/parser": "8.5.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-playwright": "^0.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.7.0",
    "eslint-plugin-sonarjs": "^0.14.0",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-testing-library": "^5.5.1",
    "globals": "15.9.0",
    "prettier": "^2.7.1",
    "typescript-eslint": "8.5.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@devlaunchers/tailwind": "workspace:*",
    "@types/espree": "10.1.0",
    "@types/jest": "28.1.6",
    "@types/node": "18.0.6",
    "@types/prettier": "2.7.0",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.15",
    "eslint": "8.21.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "3.0.2",
    "typescript": "4.7.4"
  }
}
