{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "lib": ["es5", "esnext", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "./",
    "typeRoots": ["./"],
    "types": ["node"],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  }
}
