{
  "compilerOptions": {
    "module": "commonjs",
    "jsx": "react-jsx",
    "declaration": true,
    "lib": ["es5"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
